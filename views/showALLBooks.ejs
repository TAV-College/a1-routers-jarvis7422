<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
<table>  
        <thead>
            <tr>
                <th>author</th>
                <th>title</th>
                <th>isbn</th>
            </tr>
        </thead>
        <tbody>
                <% books.forEach(function(book) { %>
                    <tr>
                        <td><%= book.author%></td>
                        <td><%= book.title %></td>
                        <td><%= book.isbn%></td>
                    </tr>
                <%});%>
        </tbody>
    </table>

</body>
</html>